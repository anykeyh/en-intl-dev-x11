!xmodmap:  up to 4 keys per modifier, (keycodes in parentheses):

! Map AltGr to the left
keycode 108 = Alt_L Meta_L
keycode 64 = ISO_Level3_Shift Alt_R Meta_R
clear Mod5
clear Mod1
add Mod5 = Alt_R Meta_R ISO_Level3_Shift Mode_switch
add Mod1 = Alt_L Meta_L

